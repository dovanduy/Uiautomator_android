			<Attribute>.+et_search.*</Attribute>
			<Method_sel>id</Method_sel>
		</Widget>
		<Widget name="match1">
			<!--中诚信征信对应的模糊匹配结果 -->
			<Attribute>中诚信征信有限公司</Attribute>
			<Method_sel>text</Method_sel>
		</Widget>
		<Widget name="match2">
			<!--中诚信征信对应的高管信息结果 孔令强 -->
			<Attribute>孔令强</Attribute>
			<Method_sel>text</Method_sel>
		</Widget>
		<Widget name="match3">
			<!--作为股东信息中的企业状态 -->
			<Attribute>吊销企业</Attribute>
			<Method_sel>text</Method_sel>
		</Widget>
		<Widget name="register">
			<!--企业报告中的注册信息 -->
			<Attribute>注册信息</Attribute>
			<Method_sel>text</Method_sel>
		</Widget>
		<Widget name="credit_code">
			<!-- ZCX统一社会信用代码-->
			<Attribute>911100007725523312</Attribute>
			<Method_sel>text</Method_sel>
		</Widget>
		<Widget name="manager">
			<!-- 企业报告中的高管信息-->
			<Attribute>高管信息</Attribute>
			<Method_sel>text</Method_sel>
		</Widget>
		<Widget name="holder">
			<!-- 个人报告中 作为股东信息-->
			<Attribute>作为股东信息</Attribute>
			<Method_sel>text</Method_sel>
		</Widget>
		<Widget name="0636">
			<!-- 查失信人、被执行人模糊匹配结果之一-->
			<Attribute>4401051983****0636</Attribute>
			<Method_sel>text</Method_sel>
		</Widget>
