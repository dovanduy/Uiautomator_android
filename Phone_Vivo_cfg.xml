			<Method_sel>text</Method_sel>
		</Widget>
		<Widget name="creditcard_banner">
			<!-- 信用卡界面的banner-->
			<Attribute>.+banner_view</Attribute>
			<Method_sel>id</Method_sel>
		</Widget>
		<Widget name="loan_banner">
			<!-- 借款界面的banner-->
			<Attribute>.+iv_banner</Attribute>
			<Method_sel>id</Method_sel>
		</Widget>
		<Widget name="banner_name">
			<!-- 信用卡banner详情中的 姓名/H5第一期-->
			<Attribute>小信扒一扒</Attribute>
			<Method_sel>text</Method_sel>
		</Widget>
		<Widget name="banner_name1">
			<!-- 信用卡banner详情中的 姓名/千面真我-->
			<Attribute>我有千面 就要不凡</Attribute>
			<Method_sel>desc</Method_sel>
		</Widget>
		<Widget name="banner_phone">
			<!-- 借款banner详情中的 手机号码/H5第二期-->
			<Attribute>小信扒一扒</Attribute>
			<Method_sel>desc</Method_sel>
		</Widget>
