		<Widget name="upload">
			<!--升级版本按钮 -->
			<Attribute>.+upgrade_confirm_btn.*</Attribute>
			<Method_sel>id</Method_sel>
		</Widget>
		<Widget name="ok1">
			<!--替换应用中的确定 -->
			<Attribute>确定</Attribute>
			<Method_sel>text</Method_sel>
		</Widget>
		<Widget name="install">
			<!--万象应用安装 -->
			<Attribute>安装</Attribute>
			<Method_sel>text</Method_sel>
		</Widget>
		<Widget name="open">
			<!--万象应用打开 -->
			<Attribute>打开</Attribute>
			<Method_sel>text</Method_sel>
		</Widget>
		<Widget name="allow">
			<!--万象应用权限获取 -->
			<Attribute>允许</Attribute>
			<Method_sel>text</Method_sel>
		</Widget>
		<Widget name="first_page">
			<!--万象应用第一页首页 -->
			<Attribute>首页</Attribute>
			<Method_sel>text</Method_sel>
		</Widget>
		<Widget name="second_page">
			<!--万象应用第二页发现 -->
			<Attribute>发现</Attribute>
			<Method_sel>text</Method_sel>
		</Widget>
		<Widget name="third_page">
			<!--万象应用第三页信用圈 -->
			<Attribute>信用圈</Attribute>
			<Method_sel>text</Method_sel>
		</Widget>
		<Widget name="fourth_page">
			<!--万象应用第四页我的 -->
			<Attribute>我的</Attribute>
			<Method_sel>text</Method_sel>
		</Widget>
