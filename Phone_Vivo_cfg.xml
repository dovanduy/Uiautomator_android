			<!--首页的企业报告 -->
			<Attribute>企业报告</Attribute>
			<Method_sel>text</Method_sel>
		</Widget>
		<Widget name="dishonest">
			<!--首页的查失信人 -->
			<Attribute>查失信人</Attribute>
			<Method_sel>text</Method_sel>
		</Widget>
		<Widget name="enforce">
			<!--首页的被执行人 -->
			<Attribute>被执行人</Attribute>
			<Method_sel>text</Method_sel>
		</Widget>
